<% if @model.hasMinMaxValidations(): %>
  <% min = @model.get('min') %>
  <% max = @model.get('max') %>

  <div class='fr_min_max'>
    <% if min && max: %>
      <%= FormRenderer.t.enter_between.replace(':min', min).replace(':max', max) %>.
    <% else if min: %>
      <%= FormRenderer.t.enter_at_least.replace(':min', min) %>.
    <% else if max: %>
      <%= FormRenderer.t.enter_up_to.replace(':max', max) %>.
    <% end %>
  </div>
<% end %>
